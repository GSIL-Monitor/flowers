page {
    height: 100%;
    width: 100%;
}
.delect{
    position: absolute;
    height: 110rpx;
    width: 110rpx;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, .5)
}
.detail {
    background: #fff!important;
}
.product{
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    height: 100rpx;
    background: #eeeeee;
    border-bottom: 1px solid #ccc;
}
.product text{
    width: 20%;
    text-align: center;
}